<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="courseschesystem.entity.Student" table="student" schema="manage_system">
        <id name="stuid" column="stuid"/>
        <property name="stuname" column="stuname"/>
        <property name="stusex" column="stusex"/>
        <property name="stubirth" column="stubirth"/>
        <property name="stuIDnum" column="stuIDnum"/>
        <property name="sturace" column="sturace"/>
        <property name="stuindate" column="stuindate"/>
        <property name="classid" column="classid"/>
        <property name="majorid" column="majorid"/>
        <property name="aid" column="aid"/>
        <property name="mail" column="mail"/>
        <property name="edu" column="edu"/>

    </class>
</hibernate-mapping>