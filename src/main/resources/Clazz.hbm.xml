<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="courseschesystem.entity.Clazz" table="class" schema="manage_system">
        <id name="classid" column="classid"/>
        <property name="classname" column="classname"/>
        <property name="stunum" column="stunum"/>
        <property name="aid" column="aid"/>
        <property name="majorid" column="majorid"/>

    </class>
</hibernate-mapping>