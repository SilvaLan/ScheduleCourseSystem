<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="courseschesystem.entity.Instruction" table="instruction" schema="manage_system">
        <id name="insid" column="insid"/>
        <property name="instime" column="instime"/>
        <property name="tid" column="tid"/>
        <property name="courseid" column="courseid"/>
        <property name="rid" column="rid"/>

    </class>
</hibernate-mapping>