<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="courseschesystem.entity.Major" table="major" schema="manage_system">
        <id name="majorid" column="majorid"/>
        <property name="majorname" column="majorname"/>
        <property name="aid" column="aid"/>
        <property name="obcredit" column="obcredit"/>
        <property name="opcredit" column="opcredit"/>
        <property name="sumcredit" column="sumcredit"/>

    </class>
</hibernate-mapping>